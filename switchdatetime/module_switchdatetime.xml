<?xml version="1.0" encoding="UTF-8"?>
<project name="module_switchdatetime" default="compile.module.switchdatetime">
  <dirname property="module.switchdatetime.basedir" file="${ant.file.module_switchdatetime}"/>
  
  <property name="module.jdk.home.switchdatetime" value="${jdk.home.android_api_25_platform}"/>
  <property name="module.jdk.bin.switchdatetime" value="${jdk.bin.android_api_25_platform}"/>
  <property name="module.jdk.classpath.switchdatetime" value="jdk.classpath.android_api_25_platform"/>
  
  <property name="compiler.args.switchdatetime" value="-encoding UTF-8 -source 1.7 -target 1.7 ${compiler.args}"/>
  
  <property name="switchdatetime.output.dir" value="${module.switchdatetime.basedir}/build/intermediates/classes/debug"/>
  <property name="switchdatetime.testoutput.dir" value="${module.switchdatetime.basedir}/build/intermediates/classes/test/debug"/>
  
  <path id="switchdatetime.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="switchdatetime.module.production.classpath">
    <path refid="${module.jdk.classpath.switchdatetime}"/>
    <path refid="library.gradle:_com.android.support:appcompat-v7-25.4.0.classpath"/>
